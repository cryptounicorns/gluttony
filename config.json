{
  "Inputs": [
    {
      "Consumer": {
        "Format": "json",
        "Queue": {
          "Nsq": {
            "Addr": "127.0.0.1:4150",
            "Channel": "gluttony",
            "ConsumerBufferSize": 128,
            "LogLevel": "info",
            "Topic": "tickers"
          },
          "Type": "nsq"
        }
      },
      "Database": {
        "Influxdb": {
          "Client": {
            "Addr": "http://127.0.0.1:8086"
          },
          "Writer": {
            "Batch": {
              "FlushInterval": "5s",
              "Points": {
                "Database": "gluttony",
                "Precision": "ns",
                "RetentionPolicy": "autogen",
                "WriteConsistency": "any"
              },
              "Size": 2048
            },
            "Point": {
              "Fields": [
                "high",
                "low",
                "vol",
                "last",
                "buy",
                "sell"
              ],
              "Name": "tickers",
              "Tags": [
                "market",
                "currencyPair",
                "tags"
              ],
              "Timestamp": "timestamp",
              "TimestampPrecision": "nanosecond"
            }
          }
        },
        "Type": "influxdb"
      },
      "Preprocessor": {
        "Lua": {
          "Code": "function preprocess(v)\n  v['currencyPair'] = v['currencyPair'][1]['symbol'] ..\n    '-' .. v['currencyPair'][2]['symbol']\n  return v\nend\n",
          "FunctionName": "preprocess",
          "Workers": 128
        },
        "Type": "lua"
      }
    }
  ],
  "Logger": {
    "Formatter": "json",
    "Level": "info"
  }
}
